            <InnerShadow color="#000000a6" height="11.67" radius="5.335" width="11.67" />
         </effect>
         <padding>
            <Insets left="30.0" />
         </padding>
      </TextField>
      <Button id="register" fx:id="AlterPassword_Button" layoutX="198.0" layoutY="317.0" mnemonicParsing="false" onAction="#AlterPassword" onMouseEntered="#EnterRegister_Button" onMouseExited="#But_ColorExited" prefHeight="40.0" prefWidth="302.0" style="-fx-background-color: #48bcff;" text="一键修改密码" textFill="WHITE">
