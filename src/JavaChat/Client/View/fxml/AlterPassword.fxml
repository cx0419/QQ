            <InnerShadow color="#000000ab" height="11.67" radius="5.335" width="11.67" />
         </effect>
         <padding>
            <Insets left="30.0" />
         </padding>
      </PasswordField>
      <TextField id="name" fx:id="EmailVerificationCode_TextField" layoutX="336.0" layoutY="252.0" onKeyReleased="#KeyReleasedEmailVerificationCode_TextField" prefHeight="40.0" prefWidth="173.0" promptText="请输入验证码">
