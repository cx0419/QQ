      </Button>
      <PasswordField id="password" fx:id="NewPassword_PasswordField" layoutX="207.0" layoutY="125.0" onKeyReleased="#KeyReleasedAccountPasswordReleased_PasswordField" prefHeight="40.0" prefWidth="302.0" promptText="请输入密码">
         <effect>
            <InnerShadow color="#000000ab" height="11.67" radius="5.335" width="11.67" />
         </effect>
         <padding>
            <Insets left="30.0" />
         </padding>
      </PasswordField>
      <PasswordField id="password" fx:id="NewPasswordAgain_PasswordField" layoutX="207.0" layoutY="189.0" onKeyReleased="#KeyReleasedAccountPasswordAgain_PasswordField" prefHeight="40.0" prefWidth="302.0" promptText="请再次输入密码">
